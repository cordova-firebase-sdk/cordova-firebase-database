<?xml version='1.0' encoding='utf-8'?>
<plugin id="cordova-firebase-database" version="0.0.1" xmlns="http://apache.org/cordova/ns/plugins/1.0" xmlns:android="http://schemas.android.com/apk/res/android">
  <name>cordova-firebase-database</name>
  <js-module name="cordova-firebase-database" src="www/cordova-firebase-database.js">
    <clobbers target="plugin.firebase.database"/>
  </js-module>
  <js-module name="Common" src="www/common.js" />
  <js-module name="BaseClass" src="www/BaseClass.js" />
  <js-module name="BaseArrayClass" src="www/BaseArrayClass.js" />
  <js-module name="Reference" src="www/Reference.js" />
  <js-module name="Query" src="www/Query.js" />
  <js-module name="DataSnapshot" src="www/DataSnapshot.js" />
  <js-module name="FirebaseDatabaseCommandQueue" src="www/firebase-database-command-queue.js" />
  <js-module name="LZString" src="www/lz-string.js" />
  <js-module name="OnDisconnect" src="www/OnDisconnect.js" />

  <platform name="browser">
    <js-module name="CordovaFirebaseDatabase" src="src/browser/CordovaFirebaseDatabase.js">
      <runs/>
    </js-module>
    <js-module name="Database" src="src/browser/Database.js" />
  </platform>

</plugin>
